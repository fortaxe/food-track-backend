{
    "version": 2,
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "methods": [
                "OPTIONS"
            ],
            "dest": "/api/index.ts"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.ts"
        }
    ]
}